[core]
    excludesfile = /Users/yuki.ieiri/.gitignore_global

[user]

[alias]
    br = branch
    ci = commit
    co = checkout
    st = status
    loga = log --oneline --graph --decorate
    logo = log --oneline
    logall = log --oneline --all --graph --decorate
    rog = reflog
[merge]
    ff = true
[pull]
    ff = only
