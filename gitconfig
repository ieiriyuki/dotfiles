[alias]
    br = branch -vv
    ci = commit
    co = checkout
    df = diff
    dfc = diff --cached
    feto = fetch -p origin
    loga = log --all --oneline --graph --decorate
    logo = log --oneline --graph --decorate
    ls = ls-files
    onto = rebase --onto
    rog = reflog
    st = status
[merge]
    ff = true
[pull]
    ff = only
